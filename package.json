{
	"name": "@serverless-toolkit/cli",
	"version": "2.5.3",
	"description": "Serverless application development runtime that deploys to your AWS account using the AWS-CDK.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/serverless-toolkit/cli.git"
	},
	"bugs": {
		"url": "https://github.com/serverless-toolkit/cli/issues"
	},
	"homepage": "https://github.com/serverless-toolkit/cli#readme",
	"license": "MIT",
	"keywords": [
		"aws",
		"cdk",
		"worker",
		"saga",
		"markdown",
		"svelte"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"bin": {
		"stk": ".build/cli/index.js"
	},
	"main": ".build/stacks/index.js",
	"types": ".build/stacks/index.d.ts",
	"scripts": {
		"test": "playwright test",
		"build": "tsc",
		"cli": "yarn build && node .build/cli",
		"bootstrap": "npm run build && cdk deploy --app .build/stacks/app.js \"*\" --require-approval never --outputs-file cdk.outputs.json",
		"destroy": "cdk destroy --app .build/stacks/app.js \"*\" --force"
	},
	"devDependencies": {
		"@playwright/test": "1.25.1",
		"@types/cli-spinner": "0.2.1",
		"@types/node": "18.7.13",
		"@types/uuid": "8.3.4",
		"@types/yargs": "17.0.11",
		"aws-lambda": "1.0.7",
		"lambda-multipart-parser": "1.0.1",
		"mime-types": "2.1.35",
		"prettier": "2.7.1",
		"typescript": "4.8.2",
		"uuid": "8.3.2",
		"vm2": "3.9.10"
	},
	"dependencies": {
		"@aws-cdk/aws-apigatewayv2-alpha": "2.39.0-alpha.0",
		"@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.39.0-alpha.0",
		"aws-cdk": "2.39.0",
		"aws-cdk-exec": "0.0.5",
		"aws-cdk-lib": "2.39.0",
		"aws-sdk": "2.1204.0",
		"chokidar": "3.5.3",
		"cli-spinner": "0.2.10",
		"constructs": "10.1.88",
		"dotenv": "16.0.1",
		"enquirer": "2.3.6",
		"esbuild": "0.14.54",
		"esbuild-node-externals": "1.4.1",
		"esbuild-svelte": "0.7.1",
		"mdsvex": "0.10.6",
		"npm-programmatic": "0.0.12",
		"playwright": "1.25.1",
		"svelte": "3.49.0",
		"svelte-preprocess": "4.10.7",
		"ws-reconnect": "1.0.7",
		"yargs": "17.5.1"
	}
}
