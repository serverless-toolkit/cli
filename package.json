{
	"name": "@serverless-toolkit/cli",
	"version": "2.6.69",
	"description": "Serverless application development runtime that deploys to your AWS account using the AWS-CDK.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/serverless-toolkit/cli.git"
	},
	"bugs": {
		"url": "https://github.com/serverless-toolkit/cli/issues"
	},
	"homepage": "https://github.com/serverless-toolkit/cli#readme",
	"license": "MIT",
	"keywords": [
		"aws",
		"cdk",
		"worker",
		"saga",
		"markdown",
		"svelte",
		"serverless"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"bin": {
		"stk": ".build/cli/index.js"
	},
	"main": ".build/lib/index.js",
	"types": ".build/lib/types.d.ts",
	"scripts": {
		"test": "npm run build:preview && DEBUG=pw:webserver playwright test",
		"preview": "npm run build:preview && node preview/server.js",
		"build": "npm run build:libs && npm run build:stacks",
		"build:libs": "tsc",
		"build:stacks": " tsc --project tsconfig.stacks.json",
		"build:preview": " tsc --project tsconfig.preview.json",
		"prepublishOnly": "npm run test && npm version patch && npm run build",
		"cli": "npm run build:libs && node .build/cli"
	},
	"devDependencies": {
		"@playwright/test": "1.25.1",
		"@types/aws-lambda": "8.10.102",
		"@types/cli-spinner": "0.2.1",
		"@types/express": "^4.17.13",
		"@types/mime-types": "^2.1.1",
		"@types/node": "18.7.14",
		"@types/uuid": "8.3.4",
		"@types/yargs": "17.0.12",
		"aws-lambda": "1.0.7",
		"body-parser": "1.20.0",
		"express": "4.18.1",
		"lambda-local": "2.0.2",
		"lambda-multipart-parser": "1.0.1",
		"mime-types": "2.1.35",
		"odottaa": "1.1.7",
		"prettier": "2.7.1",
		"uuid": "8.3.2",
		"vm2": "3.9.11"
	},
	"dependencies": {
		"@aws-cdk/aws-apigatewayv2-alpha": "2.40.0-alpha.0",
		"@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.40.0-alpha.0",
		"aws-cdk": "2.40.0",
		"aws-cdk-lib": "2.40.0",
		"aws-sdk": "2.1207.0",
		"chokidar": "3.5.3",
		"cli-spinner": "0.2.10",
		"constructs": "10.1.93",
		"dotenv": "16.0.2",
		"enquirer": "2.3.6",
		"esbuild": "0.15.6",
		"esbuild-node-externals": "1.5.0",
		"esbuild-svelte": "0.7.1",
		"mdsvex": "0.10.6",
		"playwright": "1.25.1",
		"svelte": "3.49.0",
		"svelte-preprocess": "4.10.7",
		"typescript": "4.8.2",
		"update-dotenv": "1.1.1",
		"ws-reconnect": "1.0.7",
		"yargs": "17.5.1"
	}
}
