{
	"name": "@serverless-toolkit/cli",
	"version": "2.1.28",
	"engines": {
		"node": ">=16.0.0"
	},
	"bin": {
		"stk": ".build/cli/index.js"
	},
	"scripts": {
		"test": "playwright test",
		"build": "tsc",
		"cli": "yarn build && node .build/cli",
		"bootstrap": "npm run build && cdk deploy --app .build/stacks/app.js \"*\" --require-approval never --outputs-file cdk.outputs.json",
		"destroy": "cdk destroy --app .build/stacks/app.js \"*\" --force",
		"run:saga-simple": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-simple.json --envfile .env --timeout 10000",
		"run:saga-create": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-create.json --envfile .env --timeout 10000",
		"run:saga-update-increment": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-update-increment.json --envfile .env --timeout 10000",
		"run:saga-update-decrement": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-update-decrement.json --envfile .env --timeout 10000",
		"run:saga-scheduler": "npm run build && lambda-local -l .build/scheduler/index.js -e local-tests/scheduler.json --envfile .env --timeout 10000",
		"run:saga-schedule": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-schedule.json --envfile .env --timeout 10000",
		"run:saga-schedule-clear": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-schedule-clear.json --envfile .env --timeout 10000",
		"run:saga-update-message": "npm run build && lambda-local -l .build/saga/index.js -e local-tests/saga-update-message.json --envfile .env --timeout 10000",
		"run:pages-index": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-index.json --envfile .env --timeout 10000",
		"run:pages-page1": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page1.json --envfile .env --timeout 10000",
		"run:pages-page2": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page2.json --envfile .env --timeout 10000",
		"run:pages-page3": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page3.json --envfile .env --timeout 10000",
		"run:pages-page4": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page4.json --envfile .env --timeout 10000",
		"run:pages-page5": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page5.json --envfile .env --timeout 10000",
		"run:pages-page6": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page6.json --envfile .env --timeout 10000",
		"run:pages-page7": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-page7.json --envfile .env --timeout 10000",
		"run:pages-form1": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-form1.json --envfile .env --timeout 10000",
		"run:pages-form1-submit": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-form1-submit.json --envfile .env --timeout 10000",
		"run:pages-form2": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-form2.json --envfile .env --timeout 10000",
		"run:pages-form2-submit": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-form2-submit.json --envfile .env --timeout 10000",
		"run:pages-image1": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-image1.json --envfile .env --timeout 10000",
		"run:pages-css1": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-css1.json --envfile .env --timeout 10000",
		"run:pages-static1": "npm run build && lambda-local -l .build/page/index.js -e local-tests/pages-static1.json --envfile .env --timeout 10000",
		"run:workers-task1": "npm run build && lambda-local -l .build/worker/index.js -e local-tests/workers-task1.json --envfile .env --timeout 10000",
		"run:workers-task2": "npm run build && lambda-local -l .build/worker/index.js -e local-tests/workers-task2.json --envfile .env --timeout 10000",
		"run:workers-task3": "npm run build && lambda-local -l .build/worker/index.js -e local-tests/workers-task3.json --envfile .env --timeout 10000"
	},
	"devDependencies": {
		"@playwright/test": "1.23.4",
		"@types/node": "18.0.5",
		"@types/randomstring": "1.1.8",
		"@types/uuid": "8.3.4",
		"aws-lambda": "1.0.7",
		"lambda-local": "2.0.2",
		"lambda-multipart-parser": "1.0.1",
		"mime-types": "2.1.35",
		"prettier": "2.7.1",
		"randomstring": "1.2.2",
		"typescript": "4.7.4",
		"uuid": "8.3.2",
		"vm2": "3.9.10"
	},
	"dependencies": {
		"@aws-cdk/aws-apigatewayv2": "1.164.0",
		"@aws-cdk/aws-apigatewayv2-integrations": "1.164.0",
		"@aws-cdk/aws-certificatemanager": "1.164.0",
		"@aws-cdk/aws-dynamodb": "1.164.0",
		"@aws-cdk/aws-events": "1.164.0",
		"@aws-cdk/aws-events-targets": "1.164.0",
		"@aws-cdk/aws-iam": "1.164.0",
		"@aws-cdk/aws-lambda": "1.164.0",
		"@aws-cdk/aws-lambda-nodejs": "1.164.0",
		"@aws-cdk/aws-route53": "1.164.0",
		"@aws-cdk/aws-s3": "1.164.0",
		"@aws-cdk/core": "1.164.0",
		"aws-cdk": "2.32.1",
		"aws-cdk-exec": "0.0.5",
		"aws-sdk": "2.1176.0",
		"chokidar": "3.5.3",
		"enquirer": "2.3.6",
		"esbuild": "0.14.49",
		"esbuild-node-externals": "1.4.1",
		"esbuild-svelte": "0.7.1",
		"mdsvex": "0.10.6",
		"npm-programmatic": "0.0.12",
		"odottaa": "1.1.7",
		"playwright": "1.23.4",
		"svelte": "3.49.0",
		"svelte-preprocess": "4.10.7",
		"ws-reconnect": "1.0.7",
		"yargs": "17.5.1"
	},
	"stk": {
		"domainName": "serverless-toolkit.com"
	}
}
